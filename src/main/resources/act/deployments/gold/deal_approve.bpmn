  <signal id="dataChange" name="复核数据变更" activiti:scope="processInstance"></signal>
  <signal id="makesure" name="复核通过" activiti:scope="processInstance"></signal>
  <signal id="toDelivery" name="准备去交割" activiti:scope="global"></signal>
  <process id="deal_approve" name="交易复核放行流程" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="recheck" name="复核" activiti:candidateUsers="${recheckUsers}" activiti:candidateGroups="${recheckGroup}"></userTask>
    <userTask id="approve" name="放行" activiti:candidateUsers="${approveUsers}" activiti:candidateGroups="${approveGroup}"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="recheck"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="startevent1" targetRef="approve"></sequenceFlow>
    <endEvent id="terminateendevent1" name="TerminateEndEvent">
      <terminateEventDefinition></terminateEventDefinition>
    </endEvent>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="recheck" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow4" name="取消终止" sourceRef="exclusivegateway1" targetRef="terminateendevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${recheckFlag=='stop'}]]></conditionExpression>
    </sequenceFlow>
    <intermediateThrowEvent id="signalintermediatethrowevent2" name="SignalThrowEvent">
      <signalEventDefinition signalRef="makesure"></signalEventDefinition>
    </intermediateThrowEvent>
    <sequenceFlow id="flow7" name="确认" sourceRef="exclusivegateway1" targetRef="signalintermediatethrowevent2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${recheckFlag=='rechecked'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="signalintermediatethrowevent2" targetRef="endevent1"></sequenceFlow>
    <serviceTask id="servicetask3" name="放行出账" activiti:class="org.liwang.service.ApproveActService"></serviceTask>
    <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow12" sourceRef="servicetask3" targetRef="exclusivegateway4"></sequenceFlow>
    <sequenceFlow id="flow13" name="已交割" sourceRef="exclusivegateway4" targetRef="terminateendevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${accountFlag=='deliveried'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow20" name="未交割" sourceRef="exclusivegateway4" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${accountFlag=='noDeliveried'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow21" sourceRef="servicetask4" targetRef="terminateendevent1"></sequenceFlow>
    <userTask id="usertask3" name="等待"></userTask>
    <boundaryEvent id="boundarysignal6" name="Signal" attachedToRef="usertask3" cancelActivity="true">
      <signalEventDefinition signalRef="dataChange"></signalEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow22" name="修改过" sourceRef="boundarysignal6" targetRef="approve"></sequenceFlow>
    <serviceTask id="servicetask4" name="启动到期交割流程" activiti:class="org.liwang.service.DeliveryActService"></serviceTask>
    <intermediateThrowEvent id="signalintermediatethrowevent3" name="SignalThrowEvent">
      <signalEventDefinition signalRef="dataChange"></signalEventDefinition>
    </intermediateThrowEvent>
    <sequenceFlow id="flow23" name="修改" sourceRef="exclusivegateway1" targetRef="signalintermediatethrowevent3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${recheckFlag=='update'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow24" sourceRef="signalintermediatethrowevent3" targetRef="recheck"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway5" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow25" sourceRef="approve" targetRef="exclusivegateway5"></sequenceFlow>
    <sequenceFlow id="flow26" name="已复核已出账" sourceRef="exclusivegateway5" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approveFlag=='approve' && recheckFlag=='rechecked'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow27" name="已放行" sourceRef="exclusivegateway5" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approveFlag=='approve'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow28" name="踢回" sourceRef="exclusivegateway5" targetRef="recheck">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approveFlag=='reback'}]]></conditionExpression>
    </sequenceFlow>
    <boundaryEvent id="boundarysignal7" name="Signal" attachedToRef="usertask3" cancelActivity="true">
      <signalEventDefinition signalRef="makesure"></signalEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow29" name="已复核" sourceRef="boundarysignal7" targetRef="servicetask3"></sequenceFlow>
    <boundaryEvent id="boundaryerror1" name="放行异常" attachedToRef="servicetask3">
      <errorEventDefinition errorRef="approveError"></errorEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow30" name="放行失败" sourceRef="boundaryerror1" targetRef="approve"></sequenceFlow>
  </process>
